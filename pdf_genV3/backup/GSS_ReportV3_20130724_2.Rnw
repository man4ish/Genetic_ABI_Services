# push pod definition
push <- function(vec, item) {
   	vec=substitute(vec)
   	if (is(item, "character")) 
   	{
      		eval.parent(parse(text = paste(vec, ' <- c(', vec, ', "', item, '")', sep = '')), n = 1)
   	} 
   	else 
   	{
      		eval.parent(parse(text = paste(vec, ' <- c(', vec, ', ', item, ')', sep = '')), n = 1)
   	}
}

pop <- function(vec) {
  	tmp <- vec[length(vec)]
  	vec=substitute(vec)
  	eval.parent(parse(text = paste(vec, ' <- ', vec, '[-length(', vec, ')]',
	sep = '')),
	n = 1)
	tmp
}

